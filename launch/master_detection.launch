<launch>

	<!--UEYE Node-->
	<include file="$(find ueye)/launch/nodelets-40fps.launch">
	</include>


	<!--Detector Node-->
	<include file="$(find apriltag_ros)/launch/detector.launch">
		<arg name="camera" value="/camera"/>
		<arg name="size" value="0.198"/>
	</include>


	<node name="itech_ros_io" pkg="itech_ros_io" type="marker_pos.py"/>


	<node name="itech_ros_io" pkg="itech_ros_io" type="marker_pos.py">
		<param name="~topic" value="/itech_ros/marker_pos/pose_corrected"/>
		<param name="~ip" value="192.168.1.100"/>
		<param name="~port" value="8080"/>
	</node>

</launch>
